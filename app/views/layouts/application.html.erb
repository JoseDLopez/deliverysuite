<!DOCTYPE html>
<html>
  <head>
    <title>Deliverysuite</title>
      <%= csrf_meta_tags %>

      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">

      <div class="row">
        <div class="col-md-12">
          <%= notice %>
          <%= alert %>
        </div>
      </div>

      <div class="row">
        <div class="col-md-12">
          <% if employee_signed_in? %>
            <%= render 'layouts/navbar' %>  
          <% end %>
        </div>
      </div>

      <%= yield %>

    </div>
  </body>
</html>
